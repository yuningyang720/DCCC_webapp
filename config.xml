<?xml version='1.0' encoding='utf-8'?>
<widget id="org.healthscitech.unite" version="1.5.4" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>
        Unite App
    </name>
    <description>
        A highly configurable questionnaire app.
    </description>
    <author email="slabbaf@uci.edu" href="http://cordova.io">
        Sina Labbaf
    </author>
    <content src="index.html" />
    <access origin="*" />
    <access origin="cdvfile://*" />
    <access origin="http://139.162.176.236/*" />
    <access origin="http://45.79.67.15/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="http://139.162.176.236/*" />
    <allow-intent href="http://45.79.67.15/*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="src/assets/img/icons/logo-36x36.png" />
        <icon density="mdpi" src="src/assets/img/icons/logo-48x48.png" />
        <icon density="hdpi" src="src/assets/img/icons/logo-72x72.png" />
        <icon density="xhdpi" src="src/assets/img/icons/logo-96x96.png" />
        <icon density="xxhdpi" src="src/assets/img/icons/logo-144x144.png" />
        <icon density="xxxhdpi" src="src/assets/img/icons/logo-192x192.png" />
        <splash density="ldpi" src="graphics/screen/android/splash-200x320.png" />
        <splash density="mdpi" src="graphics/screen/android/splash-320x480.png" />
        <splash density="hdpi" src="graphics/screen/android/splash-480x800.png" />
        <splash density="xhdpi" src="graphics/screen/android/splash-720x1280.png" />
        <splash density="xxhdpi" src="graphics/screen/android/splash-960x1600.png" />
        <splash density="xxxhdpi" src="graphics/screen/android/splash-1280x1920.png" />
        <splash density="port-ldpi" src="graphics/screen/android/splash-200x320.png" />
        <splash density="port-mdpi" src="graphics/screen/android/splash-320x480.png" />
        <splash density="port-hdpi" src="graphics/screen/android/splash-480x800.png" />
        <splash density="port-xhdpi" src="graphics/screen/android/splash-720x1280.png" />
        <splash density="port-xxhdpi" src="graphics/screen/android/splash-960x1600.png" />
        <splash density="port-xxxhdpi" src="graphics/screen/android/splash-1280x1920.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <edit-config file="*-Info.plist" mode="overwrite" target="NSLocationAlwaysUsageDescription">
            <string>We are using the location for tracking the notifications and analytics</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="overwrite" target="NSPhotoLibraryUsageDescription">
            <string>You can upload photos to answer some questions</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="overwrite" target="NSLocationWhenInUseUsageDescription">
            <string>We are using the location for tracking the notifications and analytics</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="overwrite" target="LSApplicationQueriesSchemes">
            <array>
                <string>oura</string>
            </array>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="NSMicrophoneUsageDescription">
            <string>need microphone access to record sounds</string>
        </edit-config>
        <icon height="20" src="src/assets/img/icons/logo-20x20.png" width="20" />
        <icon height="29" src="src/assets/img/icons/logo-29x29.png" width="29" />
        <icon height="29" src="src/assets/img/icons/logo-38x38.png" width="29" />
        <icon height="40" src="src/assets/img/icons/logo-40x40.png" width="40" />
        <icon height="44" src="src/assets/img/icons/logo-44x44.png" width="44" />
        <icon height="48" src="src/assets/img/icons/logo-48x48.png" width="48" />
        <icon height="50" src="src/assets/img/icons/logo-50x50.png" width="50" />
        <icon height="55" src="src/assets/img/icons/logo-55x55.png" width="55" />
        <icon height="57" src="src/assets/img/icons/logo-57x57.png" width="57" />
        <icon height="58" src="src/assets/img/icons/logo-58x58.png" width="58" />
        <icon height="60" src="src/assets/img/icons/logo-60x60.png" width="60" />
        <icon height="72" src="src/assets/img/icons/logo-72x72.png" width="72" />
        <icon height="76" src="src/assets/img/icons/logo-76x76.png" width="76" />
        <icon height="80" src="src/assets/img/icons/logo-80x80.png" width="80" />
        <icon height="87" src="src/assets/img/icons/logo-87x87.png" width="87" />
        <icon height="88" src="src/assets/img/icons/logo-88x88.png" width="88" />
        <icon height="100" src="src/assets/img/icons/logo-100x100.png" width="100" />
        <icon height="114" src="src/assets/img/icons/logo-114x114.png" width="114" />
        <icon height="120" src="src/assets/img/icons/logo-120x120.png" width="120" />
        <icon height="152" src="src/assets/img/icons/logo-152x152.png" width="152" />
        <icon height="167" src="src/assets/img/icons/logo-167x167.png" width="167" />
        <icon height="169" src="src/assets/img/icons/logo-169x169.png" width="169" />
        <icon height="172" src="src/assets/img/icons/logo-172x172.png" width="172" />
        <icon height="180" src="src/assets/img/icons/logo-180x180.png" width="180" />
        <icon height="196" src="src/assets/img/icons/logo-196x196.png" width="196" />
        <icon height="216" src="src/assets/img/icons/logo-216x216.png" width="216" />
        <icon height="1024" src="src/assets/img/icons/logo-1024x1024-flat.png" width="1024" />
        <splash height="480" src="graphics/screen/ios/splash-320x480.png" width="320" />
        <splash height="960" src="graphics/screen/ios/splash-640x960.png" width="640" />
        <splash height="1024" src="graphics/screen/ios/splash-768x1024.png" width="768" />
        <splash height="2048" src="graphics/screen/ios/splash-1536x2048.png" width="1536" />
        <splash height="1136" src="graphics/screen/ios/splash-640x1136.png" width="640" />
        <splash height="1334" src="graphics/screen/ios/splash-750x1334.png" width="750" />
        <splash height="2208" src="graphics/screen/ios/splash-1242x2208.png" width="1242" />
    </platform>
    <preference name="Orientation" value="portrait" />
    <preference name="BackgroundColor" value="0xffd0daff" />
    <engine name="browser" spec="^5.0.4" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.4" />
    <plugin name="cordova-plugin-device" spec="^2.0.3" />
    <plugin name="cordova-plugin-local-notification" spec="^0.9.0-beta.3" />
    <plugin name="cordova-plugin-permission" spec="^0.1.0" />
    <plugin name="cordova-plugin-media-capture" spec="^3.0.3" />
    <plugin name="cordova-plugin-file" spec="^6.0.2" />
    <plugin name="cordova-plugin-customurlscheme" spec="~5.0.1">
        <variable name="URL_SCHEME" value="unite" />
    </plugin>
    <plugin name="cordova-plugin-geolocation" spec="~4.0.2" />
    <plugin name="cordova-plugin-media" spec="^5.0.3" />
    <plugin name="onesignal-cordova-plugin" spec="~2.9.1" />
    <plugin name="com.lampa.startapp" spec="~6.1.6" />
</widget>
